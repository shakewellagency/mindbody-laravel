{
    "name": "shakewell/mindbody-laravel",
    "version": "1.0.0",
    "description": "Laravel package for Mindbody Public API v6 and Webhooks integration",
    "keywords": [
        "shakewell",
        "laravel",
        "mindbody",
        "api",
        "webhooks",
        "fitness",
        "scheduling"
    ],
    "homepage": "https://github.com/shakewellagency/mindbody-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Chora",
            "email": "developers@shakewell.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "nunomaduro/collision": "^7.8|^8.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Shakewell\\MindbodyLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shakewell\\MindbodyLaravel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-mindbody-laravel --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@composer run format"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --verbose --diff",
        "format:dry-run": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --verbose --diff --dry-run"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shakewell\\MindbodyLaravel\\MindbodyLaravelServiceProvider"
            ],
            "aliases": {
                "Mindbody": "Shakewell\\MindbodyLaravel\\Facades\\Mindbody"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}